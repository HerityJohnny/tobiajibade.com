    {{!-- overlay container for contact form --}}
    <section class="contact-form-overlay-container">
        <div class="form-error-UI">
            <span class="error-text"></span>
        </div>
        <div class="close-btn-wrapper">
            <svg id="closeBtn" xmlns="http://www.w3.org/2000/svg"
                viewBox="0 0 16 16" width="16" height="16">
                <path fill-rule="evenodd"
                    d="M3.72 3.72a.75.75 0 011.06 0L8 6.94l3.22-3.22a.75.75 0 111.06 1.06L9.06 8l3.22 3.22a.75.75 0 11-1.06 1.06L8 9.06l-3.22 3.22a.75.75 0 01-1.06-1.06L6.94 8 3.72 4.78a.75.75 0 010-1.06z">
                </path>
            </svg>
        </div>
        <div class="contact-form-container">
            <span class="form-requirement-text">Please fill out the following fields</span>
            <form action="/b/contact" method="post" class="contact-form">
                <span class="isrequired">Fields marked with (<span class="required-red">*</span>) are required</span>
                <div class="input-clip">
                    <label for="fullname">Fullname <span class="required-red">*</span></label><br>
                    <input type="text" id="fullname" name="fullname" class="required-field contact-fullname">
                </div>
                <div class="input-clip">
                    <label for="email">Email address <span
                            class="required-red">*</span></label><br>
                    <input type="email" id="email" name="email" class="required-field contact-email">
                </div>
                <div class="input-clip">
                    <label for="msg">Message <span
                            class="required-red">*</span></label><br>
                    <textarea id="msg" name="message" class="required-field contact-message" rows="4"></textarea>
                </div>
                <div class="btn-clip">
                    <button type="submit" class="contact-submit">Send message</button>
